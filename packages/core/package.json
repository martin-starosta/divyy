    "./services/YahooFinanceService": {
      "import": "./dist/services/YahooFinanceService.js",
      "types": "./dist/services/YahooFinanceService.d.ts"
    },
    "./services/AlphaVantageService": {
      "import": "./dist/services/AlphaVantageService.js",
