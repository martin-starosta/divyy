    "./services/DatabaseService": {
      "import": "./dist/services/DatabaseService.js",
      "types": "./dist/services/DatabaseService.d.ts"
    },
    "./calculators/DividendCalculator": {
      "import": "./dist/calculators/DividendCalculator.js",
      "types": "./dist/calculators/DividendCalculator.d.ts"
