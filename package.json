{
  "name": "divvy-potential-cli",
  "version": "0.1.0",
  "description": "CLI that estimates dividend yield potential from free data sources",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "divvy": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "refresh-leaderboard": "node scripts/refresh-leaderboard.js",
    "test-db": "tsx test-db-connection.js",
    "test-cache": "tsx test-caching.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.56.1",
    "commander": "^12.1.0",
    "dotenv": "^17.2.1",
    "yahoo-finance2": "^2.13.3"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
