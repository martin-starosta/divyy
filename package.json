{
  "name": "divvy-monorepo",
  "version": "0.1.0",
  "description": "Monorepo for Divvy dividend analysis CLI and web app",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=@repo/web",
    "dev:cli": "turbo dev --filter=@repo/cli",
    "build:web": "turbo build --filter=@repo/web",
    "build:cli": "turbo build --filter=@repo/cli",
    "test": "turbo test",
    "type-check": "turbo type-check",
    "lint": "turbo lint",
    "clean": "turbo clean"
  },
  "devDependencies": {
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "packageManager": "npm@10.8.1"
}
